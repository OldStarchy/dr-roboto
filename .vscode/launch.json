{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceFolder}/transpiler-dist/transpile.js"
		},
		{
			"name": "launch-lua",
			"type": "lua",
			"request": "launch",
			"workingDirectory": "${workspaceRoot}",
			"sourceBasePath": "${workspaceRoot}\\computerRoot",
			"executable": "lua.exe",
			"arguments": "debug.lua",
			"listenPublicly": false,
			"listenPort": 56789,
			"encoding": "UTF-8",
			"env": {},
			"windows": {
				"executable": "lua.exe"
			},
			"osx": {
				"executable": "/usr/local/opt/lua@5.1/bin/lua5.1"
			}
		}
	]
}
